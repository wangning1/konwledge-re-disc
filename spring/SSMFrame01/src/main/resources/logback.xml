<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>\(%d{HH:mm:ss.SSS} [%thread]\) %-5level %logger{32} %L - %msg%n</pattern>
    </encoder>
  </appender>
  <root>
    <level value="INFO"/>
    <appender-ref ref="STDOUT"/>
  </root>
  <!--<logger name="com.web" level="INFO" additivity="true">-->
    <!--<appender-ref ref="STDOUT"/>-->
  <!--</logger>-->
  <logger name="com.alibaba.druid" level="DEBUG" additivity="true"/>
</configuration>